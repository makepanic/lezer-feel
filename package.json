{
  "name": "lezer-feel",
  "version": "0.8.4",
  "description": "Lezer-based FEEL grammar",
  "main": "dist/index.cjs",
  "type": "module",
  "exports": {
    "import": "./dist/index.es.js",
    "require": "./dist/index.cjs"
  },
  "source": "src/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "author": "Nico Rehwaldt <https://github.com/nikku>",
  "license": "MIT",
  "devDependencies": {
    "@lezer/generator": "^1.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.19.0",
    "eslint-plugin-bpmn-io": "^0.14.0",
    "mocha": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.76.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@lezer/highlight": "^1.0.0",
    "@lezer/lr": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikku/lezer-feel.git"
  },
  "scripts": {
    "build": "lezer-generator src/feel.grammar -o src/parser && rollup -c",
    "build-debug": "lezer-generator src/feel.grammar --names -o src/parser && rollup -c",
    "test": "mocha test/test-*.js",
    "dev": "run-p dev:*",
    "dev:build": "chokidar 'src/*.grammar' 'src/{highlight,tokens}.js' --initial -c 'npm run build-debug'",
    "dev:test": "chokidar '{dist,test}/**/*.{js,cjs,txt}' -c 'npm test'",
    "all": "run-s lint build test:types test",
    "lint": "eslint .",
    "test:types": "tsc --noEmit test/test-types.ts",
    "prepare": "run-s build"
  },
  "files": [
    "dist"
  ]
}
